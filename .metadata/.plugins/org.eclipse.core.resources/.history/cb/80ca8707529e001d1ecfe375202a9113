function selecionar() {

	var pacote = document.getElementsByClassName('.form-check-input');
	for (var i = 0; i < pacote.length; i++) {
		if (pacote[i].checked) {
			if (pacote[i].value == "adm") {
				var Pacote_i = "adm";
				console.log("adm")
			} else if (pacote[i].value == "cli") {
				var Pacote_ii = "cli";
				console.log("cli")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("com")
			} else if (pacote[i].value == "dba") {
				var Pacote_iii = "dba";
				console.log("dba")
			} else if (pacote[i].value == "dir") {
				var Pacote_iii = "dir";
				console.log("dba")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("dba")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("dba")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("dba")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("dba")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("dba")
			}
		}
	}

}