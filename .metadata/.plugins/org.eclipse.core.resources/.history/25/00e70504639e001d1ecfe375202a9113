function selecionar() {

	var pacote = document.querySelectorAll('.form-check-input');
	for (var i = 0; i < pacote.length; i++) {
		if (pacote[i].checked) {
			if (pacote[i].value == "adm") {
				var Pacote_i = "adm";
				console.log("adm")
			} else if (pacote[i].value == "cli") {
				var Pacote_ii = "cli";
				console.log("cli")
			} else if (pacote[i].value == "com") {
				var Pacote_iii = "com";
				console.log("com")
			} else if (pacote[i].value == "dba") {
				var Pacote_iii = "dba";
				console.log("dba")
			} else if (pacote[i].value == "dir") {
				var Pacote_iii = "dir";
				console.log("dir")
			} else if (pacote[i].value == "dkt") {
				var Pacote_iii = "dkt";
				console.log("dkt")
			} else if (pacote[i].value == "inf") {
				var Pacote_iii = "inf";
				console.log("inf")
			} else if (pacote[i].value == "inv") {
				var Pacote_iii = "inv";
				console.log("inv")
			} else if (pacote[i].value == "prd") {
				var Pacote_iii = "prd";
				console.log("prd")
			} else if (pacote[i].value == "qa") {
				var Pacote_iii = "qa";
				console.log("qa")
			} else if (pacote[i].value == "rel") {
				var Pacote_iii = "rel";
				console.log("rel")
			} 
		}
	}
	
	// pega valor do option selecionado
  var valor = pacote.values()
  // muda url do navegador


}